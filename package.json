{
  "name": "bocf",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    ".": "./src/archive.mjs"
  },
  "description": "build OCI Image Format images",
  "keywords": [
    "OCI",
    "container"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "0BSD",
  "bin": {
    "bocf": "src/bocf-cli.mjs"
  },
  "scripts": {
    "test": "node --run test:ava",
    "test:ava": "ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 4m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src**/*.mjs",
    "lint": "node --run lint:docs",
    "lint:docs": "documentation lint ./src**/*.mjs"
  },
  "dependencies": {
    "commander": "^13.1.0",
    "config-expander": "^14.0.9",
    "pump": "^3.0.0",
    "tar-stream": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^6.2.0",
    "c8": "^10.1.3",
    "documentation": "^14.0.3",
    "semantic-release": "^24.2.2"
  },
  "engines": {
    "node": ">=16.17.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/bocf.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/bocf/issues"
  },
  "homepage": "https://github.com/arlac77/bocf#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-javascript-component"
    ]
  }
}
