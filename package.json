{
  "name": "bocf",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": "./src/archive.mjs"
  },
  "description": "build OCI Image Format images",
  "keywords": [
    "OCI",
    "container"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "bin": {
    "bocf": "src/bocf-cli.mjs"
  },
  "scripts": {
    "test": "npm run test:ava",
    "test:ava": "ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs",
    "cover": "c8 -x 'tests/**/*' --temp-directory build/tmp ava --timeout 2m tests/*-ava.mjs tests/*-ava-node.mjs && c8 report -r lcov -o build/coverage --temp-directory build/tmp",
    "docs": "documentation readme --section=API ./src/**/*.mjs",
    "lint": "npm run lint:docs",
    "lint:docs": "documentation lint ./src/**/*.mjs"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "config-expander": "^13.0.8",
    "pump": "^3.0.0",
    "tar-stream": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^5.3.1",
    "c8": "^8.0.0",
    "documentation": "^14.0.2",
    "semantic-release": "^21.0.5"
  },
  "engines": {
    "node": ">=16.17.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/bocf"
  },
  "bugs": {
    "url": "https://github.com/arlac77/bocf/issues"
  },
  "homepage": "https://github.com/arlac77/bocf#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-esm-only"
    ]
  }
}
